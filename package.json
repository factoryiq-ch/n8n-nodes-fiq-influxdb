{
  "name": "@factoryiq/n8n-nodes-fiq-influxdb",
  "version": "2.0.0",
  	"description": "n8n node for InfluxDB v3 using native SQL queries with advanced query management, bookmarking, and system monitoring capabilities",
  "keywords": [
    "n8n-community-node-package",
    "influxdb",
    "influxdb-v3",
    "time-series",
    "database",
    "sql",
    "monitoring",
    "industrial",
    "iot",
    "factoryiq"
  ],
  "license": "MIT",
  "homepage": "https://github.com/factoryiq/fiq-n8n",
  "author": {
    "name": "FactoryIQ",
    "email": "support@factoryiq.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/factoryiq/fiq-n8n.git"
  },
  "engines": {
    "node": ">=18.17.0",
    "pnpm": ">=8.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "npx rimraf dist && tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json",
    "test": "jest --coverage",
    "release": "standard-version",
    "release:alpha": "standard-version --prerelease alpha",
    "release:beta": "standard-version --prerelease beta"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/InfluxDbApi.credentials.js"
    ],
    		"nodes": [
			"dist/nodes/InfluxDb/InfluxDb.node.js"
		]
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@typescript-eslint/parser": "~8.32.0",
    "eslint": "^8.57.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.3",
    "gulp": "^5.0.0",
    "gulp-typescript": "^5.0.1",
    "jest": "^29.0.0",
    "n8n-workflow": "^1.2.0",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "publishConfig": {
    "access": "public"
  }
}
